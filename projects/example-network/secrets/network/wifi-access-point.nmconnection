[connection]
id=wifi-access-point
type=wifi
# do not use AP by default
autoconnect=false
interface-name=wlan0

[wifi]
# disable power management
powersave=2
mode=ap
ssid=DOTPI-AP

[wifi-security]
key-mgmt=wpa-psk
psk=apPassword

[ipv4]
method=shared

[ipv6]
addr-gen-mode=default
method=link-local

[proxy]
