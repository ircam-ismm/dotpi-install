[connection]
id=wifi-internet
type=wifi
interface-name=wlan0
autoconnect=false
autoconnect-priority=-10
# always retry to connect
autoconnect-retries=0
# always retry to authenticate
auth-retries=0
[wifi]
mode=infrastructure
ssid=ircam-public

[wifi-security]
key-mgmt=wpa-psk
psk=publicPassword


[ipv4]
method=auto

[ipv6]
addr-gen-mode=default
method=link-local

[proxy]
